{
  "verbose": true,
  "watch": ["src/**/*.ts"],
  "execMap": {
    "ts": "node --inspect=0.0.0.0:9229 --nolazy -r ts-node/register"
  },
  "legacyWatch": true,
  "env": {
    "TIMEZONE": "Asia/Ho_Chi_Minh",
    "ACCESS_TOKEN_TTL": "600",
    "REFRESH_TOKEN_TTL": "86400",
    "REDIS_URL": "redis://localhost:6379",
    "CACHE_TTL": "60",
    "DATABASE_URL2": "mongodb://admin:admin@localhost:27017/app?authSource=admin",
    "IS_REMOVE_OLD_TOKEN": "true"
  },
  "delay": "2500"
}
